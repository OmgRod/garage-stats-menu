{
	"geode": "5.2.0",
	"gd": {
		"win": "2.2081",
		"android": "2.2081",
		"mac": "2.2081",
		"ios": "2.2081"
	},
	"id": "capeling.garage-stats-menu",
	"name": "Stats Display API",
	"version": "v1.3.1-beta.3",
	"developers": ["Capeling", "OmgRod"],
	"description": "Allows mods to easily add custom stats into the icon kit",
	"api": {
		"include": [
			"include/*.h"
		]
	},
	"dependencies": {
		"geode.node-ids": ">=v1.21.1"
	},
	"incompatibilities": {
		"devcmb.cleanermenu": ">=1.0.0"
	},
	"issues": {
		"info": "If you find any issues create an issue here!",
		"url": "https://github.com/OmgRod/garage-stats-menu/issues/new"
	},
	"tags": [
		"utility",
		"developer",
		"interface",
		"api"
	],
	"settings": {
		"stars-stat": {
			"name": "Enable Stars",
			"type": "bool",
			"default": true
		},
		"moons-stat": {
			"name": "Enable Moons",
			"type": "bool",
			"default": true
		},
		"gold-coins-stat": {
			"name": "Enable Secret Coins",
			"type": "bool",
			"default": true
		},
		"user-coins-stat": {
			"name": "Enable User Coins",
			"type": "bool",
			"default": true
		},
		"orbs-stat": {
			"name": "Enable Mana Orbs",
			"type": "bool",
			"default": true
		},
		"diamonds-stat": {
			"name": "Enable Diamonds",
			"type": "bool",
			"default": true
		},
		"diamond-shards-stat": {
			"name": "Enable Diamond Shards",
			"type": "bool",
			"default": true
		}
	}
}